pipeline {
  agent any
  stages {
    stage('before') {
      steps {
        echo 'before...'
        }
    }
    stage('para') {
      parallel {
        stage('apple'){
        echo 'apple'
        }
        stage('banana'){
        echo 'banana'
        }
        stage('peach'){
        echo 'peach'
        }

    }
    stage('after') {
      steps {
        echo 'after...'
        }
    }
  }
}
